<?xml version="1.0"?>
<odoo>
    <data noupdate="1">
        <record id="group_dms_config_image_read" model="res.groups">
           <field name="category_id" ref="dms_menu.dms_role"/>
           <field name="name">DMS Config Image Read</field> 
        </record>         
        <record id="access_dms_config_image_read" model="ir.model.access">
           <field name="model_id" ref="model_dms_conf_image"/>
           <field name="name">access.dms.config.image read</field> 
           <field name="group_id" ref="group_dms_config_image_read"/>  
           <field name="perm_read">1</field> 
        </record>

        <record id="group_dms_config_image_update" model="res.groups">
            <field name="category_id" ref="dms_menu.dms_role"/>
            <field name="name">DMS Config Image Update</field> 
            <field name="implied_ids" eval="[(4, ref('group_dms_config_image_read'))]"/>
        </record>                      
        <record id="access_dms_config_image_update" model="ir.model.access">
            <field name="model_id" ref="model_dms_conf_image"/>
            <field name="name">access.dms.config.image update</field> 
            <field name="group_id" ref="group_dms_config_image_update"/>  
            <field name="perm_create">1</field> 
            <field name="perm_write">1</field> 
        </record>      

        <record id="group_dms_config_image_delete" model="res.groups">
            <field name="category_id" ref="dms_menu.dms_role"/>
            <field name="name">DMS Config Image Delete</field> 
            <field name="implied_ids" eval="[(4, ref('group_dms_config_image_read'))]"/>
        </record>        
        <record id="access_dms_config_image_delete" model="ir.model.access">
            <field name="model_id" ref="model_dms_conf_image"/>
            <field name="name">access.dms.config.image delete</field> 
            <field name="group_id" ref="group_dms_config_image_delete"/>  
            <field name="perm_unlink">1</field> 
        </record>

        </data>
    <data>
        <record id="dms_menu.group_sys_admin" model="res.groups">
            <field name="implied_ids" eval="[
                (4,ref('group_dms_config_image_read')),
                (4,ref('group_dms_config_image_update')),
                (4,ref('group_dms_config_image_delete')),
                ]" />
        </record> 
    </data>
</odoo>